-- @path SOAML=/BPMN2SOAML/OUT/SoaMLBase.ecore
-- @nsURI BPMN=http://stp.eclipse.org/bpmn

module BPMN2SOAML;
create OUT : SOAML from IN : BPMN;


rule prova {
	from
	s: BPMN!Pool
	to
	t: SOAML!Class (name <- s.name),
	p: SOAML!Participant (baseClass <- t)
}



rule prova2 {
	from
	s: BPMN!Activity
	to
	t: SOAML!Activity
}