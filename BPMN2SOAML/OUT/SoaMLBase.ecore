          <eOperations name="mustConform" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean">
            <eAnnotations source="http://www.eclipse.org/uml2/1.1.0/GenModel">
              <details key="body" value="true"/>
